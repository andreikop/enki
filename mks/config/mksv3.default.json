{
    "_version" : 2,

    "NegativeFileFilter": [ "*~", "*.o", "*.pyc", "*.bak" ], 

    "Workspace": {
        "FileSortMode": "URL"
    },
    "Editor": {
        "AutoCompletion": {
            "CaseSensitivity": true, 
            "Enabled": true, 
            "ReplaceWord": false, 
            "ShowSingle": false, 
            "Source": "Document",
            "Threshold": 3
        }, 
        "BraceMatching": {
            "Enabled": true, 
            "MatchedBackgroundColor": "#ffff7f",
            "MatchedForegroundColor": "#ff0000",
            "Mode": "Sloppy",
            "UnmatchedBackgroundColor": "#ff0000",
            "UnmatchedForegroundColor": "#ffffff"
        }, 
        "CallTips": {
            "BackgroundColor": "#ffff9b", "Enabled": false, 
            "ForegroundColor": "#000000", "HighlightColor": "#ff0000", "Style": "NoAutoCompletionContext", "VisibleCount": 1
        }, 
        "Caret": {
            "ForegroundColor": "#000000",
            "LineBackgroundColor": "#aaaaff",
            "LineVisible": true, 
            "Width": 1
        }, 
        "CreateBackupUponOpen": false, 
        "DefaultDocumentColours": false, 
        "DefaultDocumentPaper": "#ffffff",
        "DefaultDocumentPen": "#000000",
        "DefaultFont": "Monospace",
        "DefaultFontSize": 9, 
        "EOL": {
            "AutoDetect": true, 
            "Mode": "\\n"
        }, 
        "Edge": {
            "Color": "#ff0000",
            "Column": 80, 
            "Enabled": false, 
            "Mode": "Line"
        }, 
        "EnableCodeFolding": false, 
        "Indentation": {
            "AutoDetect": true, 
            "AutoIndent": true, 
            "BackspaceUnindents": true, 
            "ConvertUponOpen": false, 
            "Guides": true, 
            "GuidesBackgroundColor": "#0000ff",
            "GuidesForegroundColor": "#0000ff", 
            "TabIndents": true, 
            "TabWidth": 4, 
            "UseTabs": false, 
            "Width": 4
        }, 
        "SelectionBackgroundColor": "#bdff9b",
        "SelectionForegroundColor": "#000000",
        "ShowLineNumbers": true, 
        "WhitespaceVisibility": "Invisible",
        "Wrap": {
            "Enabled": true, 
            "EndVisualFlag": "None", "LineIndentWidth": 4, 
            "Mode": "WrapWord", "StartVisualFlag": "None"
        }
    }, 
    "MainWindow": {
        "Height": 480, 
        "Maximized": true, 
        "Width": 640, 
        "X": 0, 
        "Y": 0
    }, 
    "Associations": {
        "Bash": {
            "FileName": [ "*.sh" ],
            "FirstLine": [ "*bash", "*/sh*" ]
        }, 
        "Batch": {
            "FileName": [ "*.bat", "*.cmd" ],
            "FirstLine": []
        }, 
        "C#": {
            "FileName": [ "*.cs" ],
            "FirstLine": []
        }, 
        "C++": {
            "FileName": [ "*.h", "*.hh", "*.hpp", "*.hxx", "*.h++", "*.c", "*.cc", "*.cpp", "*.cxx", "*.c++" ],
            "FirstLine": []
        }, 
        "CMake": {
            "FileName": [ "*.cmake", "CMake*.txt" ],
            "FirstLine": []
        }, 
        "CSS": {
            "FileName": [ "*.css" ],
            "FirstLine": []
        }, 
        "D": {
            "FileName": [ "*.d" ],
            "FirstLine": []
        }, 
        "Diff": {
            "FileName": [ "*.diff", "*.patch" ],
            "FirstLine": [ "Index: *" ]
        }, 
        "Fortran": {
            "FileName": [ "*.f", "*.for\"", "*.ftn" ],
            "FirstLine": []
        }, 
        "Fortran77": {
            "FileName": [], 
            "FirstLine": []
        }, 
        "HTML": {
            "FileName": [ "*.asp",
                          "*.htm*", "*.php*", "*.phtm*", "*.rdf", "*.shtm*" ],
            "FirstLine": [ "<!doctype html*", "<html*", "<?php*"]
        }, 
        "IDL": {
            "FileName": [ "*.idl" ],
            "FirstLine": []
        }, 
        "Java": {
            "FileName": [ "*.java" ],
            "FirstLine": []
        }, 
        "JavaScript": {
            "FileName": [ "*.js", "*.json" ],
            "FirstLine": []
        }, 
        "Lua": {
            "FileName": [ "*.lua" ],
            "FirstLine": []
        }, 
        "Makefile": {
            "FileName": [ "*.mak", "*makefile", "Makefile*", "*.mk" ],
            "FirstLine": []
        }, 
        "Markdown": {
            "FileName": [ "*.md", "*.markdown"],
            "FirstLine": []
        }, 
        "POV": {
            "FileName": [ "*.pov" ],
            "FirstLine": []
        }, 
        "Pascal": {
            "FileName": [ "*.pas" ],
            "FirstLine": []
        }, 
        "Perl": {
            "FileName": [ "*.ph", "*.pl", "*.pm" ],
            "FirstLine": [ "*perl*" ]
        }, 
        "PostScript": {
            "FileName": [ "*.ps" ],
            "FirstLine": []
        }, 
        "Properties": {
            "FileName": [ "*.cfg", "*.cnf", "*.inf", "*.ini", "*.properties", "*.rc", "*.reg" ],
            "FirstLine": []
        }, 
        "Python": {
            "FileName": [ "*.ptl", "*.py", "*.py", "*.pyw", "*.pyx" ],
            "FirstLine": [ "*python*" ]
        }, 
        "Ruby": {
            "FileName": [ "*.rb", "*.rbw" ],
            "FirstLine": []
        }, 
        "SQL": {
            "FileName": [ "*.sql" ],
            "FirstLine": []
        }, 
        "Scheme": {
            "FileName": [ "*.scm", "*.ss" ],
            "FirstLine": []
        }, 
        "Spice": {
            "FileName": [], 
            "FirstLine": []
        }, 
        "TCL": {
            "FileName": [ "*.tcl" ],
            "FirstLine": []
        }, 
        "TeX": {
            "FileName": [ "*.tex", "*.aux", "*.idx", "*.sty", "*.toc" ],
            "FirstLine": [ "\\documentclass" ]
        }, 
        "VHDL": {
            "FileName": [ "*.vhdl" ],
            "FirstLine": []
        }, 
        "Verilog": {
            "FileName": [ "*.v" ],
            "FirstLine": []
        }, 
        "XML": {
            "FileName": [ "*.xml", "*.xsd", "*.xsl", "*.xslt", "*.docbook", "*.dtd", "*.svg" ],
            "FirstLine": [ "<!doctype*", "<?xml*" ]
        }, 
        "YAML": {
            "FileName": [ "*.yml" ],
            "FirstLine": []
        }
    }, 
    "FileBrowser": {
        "PopularDirs": {}
    }, 
    "Modes": {
        "Scheme": {
            "Enabled": "whenOpened",
            "InterpreterPath": "scheme"
        }
    }, 
    "SchemeIndentHelper": {
        "Enabled": true
    }
}
